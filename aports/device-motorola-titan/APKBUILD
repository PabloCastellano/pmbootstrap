}

package() {
	install -D -m644 "$srcdir/deviceinfo" \
		"$pkgdir/etc/deviceinfo"
	install -D -m644 "$srcdir"/90-android.rules \
		"$pkgdir"/etc/udev/rules.d/90-android.rules
}

